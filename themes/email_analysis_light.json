{
    "name": "Pastel Email Headers Light",
    "type": "light",
    "colors": {
        "editor.background": "#f8f8f8",
        "editor.foreground": "#333333",
        "editor.lineHighlightBackground": "#e0e0e0",
        "editorCursor.foreground": "#555555"
    },
    "tokenColors": [
        {
            "name": "Email Address",
            "scope": "entity.name.email.address",
            "settings": {
                "foreground": "#9370db"
            }
        },
        {
            "name": "Domain Address",
            "scope": "entity.name.domain.address",
            "settings": {
                "foreground": "#6495ed"
            }
        },
        {
            "name": "String",
            "scope": "string.quoted.double.email",
            "settings": {
                "foreground": "#3cb371"
            }
        },
        {
            "name": "Variable Definition",
            "scope": "variable.definition.variablecolon",
            "settings": {
                "foreground": "#daa520"
            }
        },
        {
            "name": "Variable Name",
            "scope": "variable.other.readwrite.variablecolon",
            "settings": {
                "foreground": "#ba55d3"
            }
        },
        {
            "name": "IP Address",
            "scope": "entity.name.ip.address",
            "settings": {
                "foreground": "#ffa07a"
            }
        },
        {
            "name": "Email Security Fail",
            "scope": "entity.name.emailsec.fail",
            "settings": {
                "foreground": "#cd5c5c",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Escape Character",
            "scope": "constant.character.escape.email",
            "settings": {
                "foreground": "#bdb76b"
            }
        }
    ]
}