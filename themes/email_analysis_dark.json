{
    "name": "Pastel Email Headers Dark",
    "type": "dark",
    "colors": {
        "editor.background": "#282c34",
        "editor.foreground": "#d1d5db",
        "editor.lineHighlightBackground": "#3e4451",
        "editorCursor.foreground": "#abb2bf"
    },
    "tokenColors": [
        {
            "name": "Email Address",
            "scope": "entity.name.email.address",
            "settings": {
                "foreground": "#b19cd9"
            }
        },
        {
            "name": "Domain Address",
            "scope": "entity.name.domain.address",
            "settings": {
                "foreground": "#87ceeb"
            }
        },
        {
            "name": "String",
            "scope": "string.quoted.double.email",
            "settings": {
                "foreground": "#98fb98"
            }
        },
        {
            "name": "Variable Definition",
            "scope": "variable.definition.variablecolon",
            "settings": {
                "foreground": "#f5deb3"
            }
        },
        {
            "name": "Variable Name",
            "scope": "variable.other.readwrite.variablecolon",
            "settings": {
                "foreground": "#e1bee7"
            }
        },
        {
            "name": "IP Address",
            "scope": "entity.name.ip.address",
            "settings": {
                "foreground": "#ffb366"
            }
        },
        {
            "name": "Email Security Pass",
            "scope": "entity.name.emailsec.pass",
            "settings": {
                "foreground": "#80EF80",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Email Security Fail",
            "scope": "entity.name.emailsec.fail",
            "settings": {
                "foreground": "#ff6961",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Escape Character",
            "scope": "constant.character.escape.email",
            "settings": {
                "foreground": "#f0e68c"
            }
        }
    ]
}